<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.tvpatrol"
       name="TV Patrol"
       version="0.0.1"
       provider-name="todits.xbmc">
  <requires>
    <import addon="xbmc.python" version="1.0"/>
    <import addon="script.module.pyamf" version="0.6.1"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>ABS-CBN TV Patrol</summary>
    <description>Mga Patrol ng Pilipino: Nagbabalita at naglilingkod sa Pilipino, saan man sa mundo!</description>
    <platform>all</platform>
  </extension>
</addon>
</addons>
